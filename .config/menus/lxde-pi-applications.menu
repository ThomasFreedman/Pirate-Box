<?xml version="1.0" ?>
<!DOCTYPE Menu
  PUBLIC '-//freedesktop//DTD Menu 1.0//EN'
  'http://standards.freedesktop.org/menu-spec/menu-1.0.dtd'>
<Menu>
	<Name>Applications</Name>
	<MergeFile type="parent">/etc/xdg/menus/lxde-pi-applications.menu</MergeFile>
	<Menu>
		<Name>DesktopSettings</Name>
		<Include>
			<Filename>pcmanfm-desktop-pref.desktop</Filename>
		</Include>
		<AppDir>/home/ipfs/.local/share/applications</AppDir>
		<Include>
			<Filename>lxappearance.desktop</Filename>
		</Include>
		<Exclude>
			<Filename>qt5ct.desktop</Filename>
		</Exclude>
	</Menu>
	<DefaultLayout inline="false"/>
	<Menu>
		<Name>PirateBoxTools</Name>
		<Directory>PirateBoxTools.directory</Directory>
		<Include>
			<And>
				<Category>PirateBoxToolsCategory</Category>
			</And>
		</Include>
		<Include>
			<Filename>tlsAbout.desktop</Filename>
		</Include>
		<Include>
			<Filename>tlsSetupWiz.desktop</Filename>
		</Include>
		<Include>
			<Filename>tlsIpfsDocs.desktop</Filename>
		</Include>
		<Include>
			<Filename>tlsCrtlPanl.desktop</Filename>
		</Include>
		<Include>
			<Filename>tlsPBVGrabr.desktop</Filename>
		</Include>
		<Include>
			<Filename>tlsHotspot.desktop</Filename>
		</Include>
		<Include>
			<Filename>tlsGateway.desktop</Filename>
		</Include>
		<Include>
			<Filename>tlsEjectLL.desktop</Filename>
		</Include>
		<DefaultLayout inline="false"/>
		<Menu>
			<Name>PirateBoxAdvanced</Name>
			<Directory>PirateBoxAdvanced.directory</Directory>
			<Include>
				<Filename>advSearchGUI.desktop</Filename>
			</Include>
			<Include>
				<Filename>advSearchOps.desktop</Filename>
			</Include>
			<Include>
				<Filename>advPBVGinfo.desktop</Filename>
			</Include>
			<Include>
				<Filename>advUpgrIpfs.desktop</Filename>
			</Include>
			<Include>
				<Filename>advNomadnet.desktop</Filename>
			</Include>
			<Include>
				<Filename>advReticulm.desktop</Filename>
			</Include>
			<Include>
				<Filename>advXmppMesh.desktop</Filename>
			</Include>
			<Include>
				<Filename>advYggdrasl.desktop</Filename>
			</Include>
			<Include>
				<Filename>advMoonbeam.desktop</Filename>
			</Include>
			<Layout>
				<Merge type="menus"/>
				<Filename>advSearchGUI.desktop</Filename>
				<Filename>advSearchOps.desktop</Filename>
				<Filename>advPBVGinfo.desktop</Filename>
				<Filename>advUpgrIpfs.desktop</Filename>
				<Filename>advNomadnet.desktop</Filename>
				<Filename>advReticulm.desktop</Filename>
				<Filename>advXmppMesh.desktop</Filename>
				<Filename>advYggdrasl.desktop</Filename>
				<Filename>advMoonbeam.desktop</Filename>
