			</Layout>
		</Menu>
		<Layout>
			<Merge type="menus"/>
			<Filename>tlsAbout.desktop</Filename>
			<Filename>tlsSetupWiz.desktop</Filename>
			<Filename>tlsIpfsDocs.desktop</Filename>
			<Filename>tlsCrtlPanl.desktop</Filename>
			<Filename>tlsPBVGrabr.desktop</Filename>
			<Filename>tlsHotspot.desktop</Filename>
			<Filename>tlsGateway.desktop</Filename>
			<Filename>tlsEjectLL.desktop</Filename>
		    <Separator/>
			<Menuname>PirateBoxAdvanced</Menuname>
			<Merge type="files"/>
		</Layout>
	</Menu>
	<Layout>
		<Merge type="menus"/>
		<Menuname>Development</Menuname>
		<Menuname>Education</Menuname>
		<Menuname>Office</Menuname>
		<Menuname>Internet</Menuname>
		<Menuname>PirateBoxTools</Menuname>
		<Menuname>Multimedia</Menuname>
		<Menuname>Graphics</Menuname>
		<Menuname>Games</Menuname>
		<Menuname>System</Menuname>
		<Menuname>Accessories</Menuname>
		<Menuname>Universal Access</Menuname>
		<Separator/>
		<Menuname>Help</Menuname>
		<Separator/>
		<Menuname>DesktopSettings</Menuname>
		<Merge type="files"/>
	</Layout>
	<Menu>
		<Name>System</Name>
		<AppDir>/home/ipfs/.local/share/applications</AppDir>
		<Include>
			<Filename>htop.desktop</Filename>
		</Include>
		<Include>
			<Filename>pi-gpk-log.desktop</Filename>
		</Include>
		<Include>
			<Filename>pi-gpk-update-viewer.desktop</Filename>
		</Include>
